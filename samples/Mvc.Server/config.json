{
  "ConnectionStrings": {
    "IdentityContext": "Data Source=.;Initial Catalog=AdfDb;Integrated Security=True;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "LiterateConsole"
            },
            {
              "Name": "RollingFile",
              "Args": {
                "pathFormat": "Logs/log-{Date}.txt",
                "shared": true
              }
            }
          ]
        }
      }
    ]
  }
}
